<%= form_for @archival_object, :as => "archival_object", :url => {:action => :create}, :html => {:class => 'form-horizontal'} do |f| %>
   <%= render :partial => "archival_objects/form_container", :locals => {:f => f} %>
   <div class="form-actions">
      <div class="btn-group btn-submit-group">
         <button type="submit" class="btn btn-primary">Create Archival Object</button>
         <button type="button" id="createPlusOne" class="btn btn-primary">+1</button>
      </div>
      <%= link_to "Cancel", :back, :class => "btn btn-cancel" %>
   </div>
<% end %>